public String whichTwo(Integer a, Integer b, Integer c) {
    if (a + b == c){
        return 'c';
    } else if (a + c == b){
        return 'b';
    } else if (b + c == a){
        return 'c';
    } else{
        return '';
    }

    // if (a + b == c) {
    //     return 'c';
    // } else if (a + c == b) {
    //     return 'b';
    // } else if (c + b == a) {
    //     return 'a';
    // } else {
    //     return '';
    // }
}